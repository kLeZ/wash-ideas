{
	"name": "wash-ideas",
	"version": "1.0.0",
	"description": "Brainstorming presentation dashboard",
	"main": "index.js",
	"config": {
		"webpackenv": "development"
	},
	"scripts": {
		"clean": "rimraf dist",
		"lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
		"pretest": "npm run lint",
		"test": "GIT_HTTP_MOCK_SERVER_ROOT=__fixtures__ git-http-mock-server start && jest --coverage && GIT_HTTP_MOCK_SERVER_ROOT=__fixtures__ git-http-mock-server stop",
		"prebuild": "npm run test",
		"build": "webpack --mode $npm_package_config_webpackenv",
		"prestart": "npm run build",
		"start": "npm run serve",
		"watch": "watch 'webpack --mode $npm_package_config_webpackenv' src/",
		"serve": "http-server -c-1",
		"dist-track": "git update-index --no-skip-worktree dist/*.*",
		"dist-skip": "git update-index --skip-worktree dist/*.*"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kLeZ/wash-ideas.git"
	},
	"keywords": [
		"brainstorm",
		"dashboard",
		"gui",
		"projects",
		"ideas"
	],
	"author": "kLeZ & Plague",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/kLeZ/wash-ideas/issues"
	},
	"homepage": "https://kLeZ.github.io/wash-ideas",
	"dependencies": {
		"@alienfast/i18next-loader": "^1.0.18",
		"@material-ui/core": "^1.5.1",
		"@material-ui/icons": "^2.0.3",
		"browserfs": "^1.4.3",
		"i18next": "^12.1.0",
		"inversify": "^4.13.0",
		"isomorphic-git": "^1.8.2",
		"pify": "^4.0.1",
		"prando": "^5.1.0",
		"react": "^16.8.3",
		"react-dom": "^16.8.3",
		"react-router": "^4.3.1",
		"reflect-metadata": "^0.1.13",
		"typeface-roboto": "0.0.54",
		"typescript-logging": "^0.6.3"
	},
	"devDependencies": {
		"@types/dropboxjs": "0.0.29",
		"@types/emscripten": "0.0.31",
		"@types/filesystem": "0.0.28",
		"@types/i18next": "^11.9.3",
		"@types/jest": "^23.3.14",
		"@types/material-ui": "^0.21.6",
		"@types/node": "^10.12.27",
		"@types/pify": "^3.0.2",
		"@types/react": "^16.8.5",
		"@types/react-dom": "^16.8.2",
		"@types/react-tap-event-plugin": "0.0.30",
		"css-loader": "^1.0.1",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"git-http-mock-server": "^1.2.0",
		"hard-source-webpack-plugin": "^0.13.1",
		"http-server": "^14.1.1",
		"jest": "^24.5.0",
		"license-checker": "^20.2.0",
		"node-sass": "^7.0.0",
		"progress-bar-webpack-plugin": "^1.12.1",
		"rimraf": "^2.6.3",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.22.1",
		"ts-jest": "^24.0.0",
		"ts-loader": "^4.5.0",
		"tslint": "^5.13.0",
		"typescript": "^3.3.3333",
		"webpack": "^4.29.5",
		"webpack-cli": "^3.2.3",
		"webpack-visualizer-plugin": "^0.1.11"
	}
}
