{
	"name": "wash-ideas",
	"version": "1.0.0",
	"description": "Brainstorming presentation dashboard",
	"main": "index.js",
	"config": {
		"webpackenv": "development"
	},
	"scripts": {
		"clean": "rimraf dist",
		"lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
		"pretest": "npm run lint",
		"test": "jest --coverage",
		"prebuild": "npm run test",
		"build": "webpack --mode $npm_package_config_webpackenv",
		"prestart": "npm run build",
		"start": "http-server -c-1",
		"watch": "watch 'webpack --mode $npm_package_config_webpackenv' src/",
		"dist-track": "git update-index --no-skip-worktree dist/*.*",
		"dist-skip": "git update-index --skip-worktree dist/*.*"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kLeZ/wash-ideas.git"
	},
	"keywords": [
		"brainstorm",
		"dashboard",
		"gui",
		"projects",
		"ideas"
	],
	"author": "kLeZ & Plague",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/kLeZ/wash-ideas/issues"
	},
	"homepage": "https://kLeZ.github.io/wash-ideas",
	"dependencies": {
		"@alienfast/i18next-loader": "^1.0.18",
		"@material-ui/core": "^1.5.0",
		"@material-ui/icons": "^2.0.2",
		"browserfs": "^1.4.3",
		"i18next": "^12.0.0",
		"inversify": "^4.13.0",
		"isomorphic-git": "^0.31.0",
		"pify": "^4.0.0",
		"react": "^16.4.2",
		"react-dnd": "^7.0.2",
		"react-dnd-html5-backend": "^7.0.2",
		"react-dom": "^16.4.2",
		"react-router": "^4.3.1",
		"reflect-metadata": "^0.1.12",
		"typeface-roboto": "0.0.54",
		"typescript-logging": "^0.6.2"
	},
	"devDependencies": {
		"@types/dropboxjs": "0.0.29",
		"@types/emscripten": "0.0.31",
		"@types/filesystem": "0.0.28",
		"@types/i18next": "^11.9.3",
		"@types/jest": "^23.3.1",
		"@types/material-ui": "^0.21.5",
		"@types/node": "^10.7.1",
		"@types/pify": "^3.0.2",
		"@types/react": "^16.4.11",
		"@types/react-dom": "^16.0.7",
		"@types/react-tap-event-plugin": "0.0.30",
		"css-loader": "^1.0.0",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"file-loader": "^1.1.11",
		"http-server": "^0.11.1",
		"jest": "^23.5.0",
		"license-checker": "^20.2.0",
		"node-sass": "^4.9.3",
		"progress-bar-webpack-plugin": "^1.11.0",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.22.1",
		"ts-jest": "^23.1.3",
		"ts-loader": "^4.5.0",
		"tslint": "^5.11.0",
		"typescript": "^3.0.1",
		"webpack": "^4.16.5",
		"webpack-cli": "^3.1.0",
		"webpack-visualizer-plugin": "^0.1.11"
	}
}
